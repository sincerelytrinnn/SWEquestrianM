{
  "multipart": [
    {
      "when": {
        "side": "right"
      },
      "apply": {
        "model": "swem:block/western_pole_upper"
      }
    },
    {
      "when": {
        "side": "middle"
      },
      "apply": {
        "model": "swem:block/western_pole_middle"
      }
    },
    {
      "when": {
        "side": "left"
      },
      "apply": {
        "model": "swem:block/western_pole_lower"
      }
    }
  ]
}