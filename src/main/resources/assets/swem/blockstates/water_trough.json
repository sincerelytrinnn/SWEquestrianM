{
  "multipart": [
    {
      "when": {
        "two_way": "single"
      },
      "apply": {
        "model": "swem:block/water_trough_single"
      }
    },
    {
      "when": {
        "two_way": "middle",
        "facing": "north|south"
      },
      "apply": {
        "model": "swem:block/water_trough_middle",
        "y": 90
      }
    },
    {
      "when": {
        "two_way": "middle",
        "facing": "east|west"
      },
      "apply": {
        "model": "swem:block/water_trough_middle"
      }
    },
    {
      "when": {
        "two_way": "left",
        "facing": "east"
      },
      "apply": {
        "model": "swem:block/water_trough_end"
      }
    },
    {
      "when": {
        "two_way": "left",
        "facing": "south"
      },
      "apply": {
        "model": "swem:block/water_trough_end",
        "y": 90
      }
    },
    {
      "when": {
        "two_way": "left",
        "facing": "west"
      },
      "apply": {
        "model": "swem:block/water_trough_end",
        "y": 180
      }
    },
    {
      "when": {
        "two_way": "left",
        "facing": "north"
      },
      "apply": {
        "model": "swem:block/water_trough_end",
        "y": 270
      }
    },
    {
      "when": {
        "two_way": "right",
        "facing": "east"
      },
      "apply": {
        "model": "swem:block/water_trough_end",
        "y": 180
      }
    },
    {
      "when": {
        "two_way": "right",
        "facing": "south"
      },
      "apply": {
        "model": "swem:block/water_trough_end",
        "y": 270
      }
    },
    {
      "when": {
        "two_way": "right",
        "facing": "west"
      },
      "apply": {
        "model": "swem:block/water_trough_end"
      }
    },
    {
      "when": {
        "two_way": "right",
        "facing": "north"
      },
      "apply": {
        "model": "swem:block/water_trough_end",
        "y": 90
      }
    }
  ]
}