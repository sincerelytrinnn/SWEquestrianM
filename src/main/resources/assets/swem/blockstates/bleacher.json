{
  "multipart": [
    {
      "when": {
        "type": "top"
      },
      "apply": {
        "model": "swem:block/bleacher_top"
      }
    },
    {
      "when": {
        "type": "bottom"
      },
      "apply": {
        "model": "swem:block/bleacher"
      }
    },
    {
      "when": {
        "type": "double"
      },
      "apply": {
        "model": "swem:block/bleacher_block"
      }
    }
  ]
}
