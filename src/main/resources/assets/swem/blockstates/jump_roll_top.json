{
  "multipart": [
    {
      "when": {
        "side": "left",
        "facing": "north"
      },
      "apply": {
        "model": "swem:block/rolltop_outer"
      }
    },
    {
      "when": {
        "side": "left",
        "facing": "south"
      },
      "apply": {
        "model": "swem:block/rolltop_outer_flipped"
      }
    },
    {
      "when": {
        "side": "left",
        "facing": "east|west"
      },
      "apply": {
        "model": "swem:block/rolltop_outer",
        "y": 90
      }
    },

    {
      "when": {
        "side": "middle",
        "facing": "north|south"
      },
      "apply": {
        "model": "swem:block/rolltop_middle"
      }
    },
    {
      "when": {
        "side": "middle",
        "facing": "east|west"
      },
      "apply": {
        "model": "swem:block/rolltop_middle",
        "y": 90
      }
    },

    {
      "when": {
        "side": "right",
        "facing": "north|south"
      },
      "apply": {
        "model": "swem:block/rolltop_middle"
      }
    },
    {
      "when": {
        "side": "right",
        "facing": "east|west"
      },
      "apply": {
        "model": "swem:block/rolltop_middle"
      }
    }
  ]
}