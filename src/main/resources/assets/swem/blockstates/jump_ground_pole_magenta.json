{
  "multipart": [
    {
      "when": {
        "side": "right|left|middle",
        "facing": "north|south"
      },
      "apply": {
        "model": "swem:block/ground_pole_magenta"
      }
    },
    {
      "when": {
        "side": "right|left|middle",
        "facing": "east|west"
      },
      "apply": {
        "model": "swem:block/ground_pole_magenta",
        "y": 90
      }
    }
  ]
}