{
  "multipart": [
    {
      "when": {
        "level": "2"
      },
      "apply": {
        "model": "swem:block/full_feeder"
      }
    },
    {
      "when": {
        "level": "1"
      },
      "apply": {
        "model": "swem:block/half_feeder"
      }
    },
    {
      "when": {
        "north": "true",
        "west": "true",
        "south": "false",
        "east": "false"
      },
      "apply": {
        "model": "swem:block/slow_feeder_corner_gray"
      }
    },
    {
      "when": {
        "north": "true",
        "west": "false",
        "south": "false",
        "east": "true"
      },
      "apply": {
        "model": "swem:block/slow_feeder_corner_gray",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false",
        "west": "false",
        "south": "true",
        "east": "true"
      },
      "apply": {
        "model": "swem:block/slow_feeder_corner_gray",
        "y": 180
      }
    },
    {
      "when": {
        "north": "false",
        "west": "true",
        "south": "true",
        "east": "false"
      },
      "apply": {
        "model": "swem:block/slow_feeder_corner_gray",
        "y": 270
      }
    },
    {
      "when": {
        "north": "false",
        "west": "false|true",
        "south": "false",
        "east": "true|false"
      },
      "apply": {
        "model": "swem:block/slow_feeder_gray"
      }
    },
    {
      "when": {
        "north": "true|false",
        "west": "false",
        "south": "true|false",
        "east": "false"
      },
      "apply": {
        "model": "swem:block/slow_feeder_gray"
      }
    },
    {
      "when": {
        "north": "false",
        "west": "true|false",
        "south": "false",
        "east": "false"
      },
      "apply": {
        "model": "swem:block/slow_feeder_gray"
      }
    },
    {
      "when": {
        "north": "true",
        "west": "false",
        "south": "true",
        "east": "true"
      },
      "apply": {
        "model": "swem:block/slow_feeder_gray"
      }
    },
    {
      "when": {
        "north": "false",
        "west": "true",
        "south": "true",
        "east": "true"
      },
      "apply": {
        "model": "swem:block/slow_feeder_gray"
      }
    },
    {
      "when": {
        "north": "true",
        "west": "true",
        "south": "true",
        "east": "false"
      },
      "apply": {
        "model": "swem:block/slow_feeder_gray"
      }
    },
    {
      "when": {
        "north": "true",
        "west": "true",
        "south": "false",
        "east": "true"
      },
      "apply": {
        "model": "swem:block/slow_feeder_gray"
      }
    },
    {
      "when": {
        "north": "true",
        "west": "true",
        "south": "true",
        "east": "true"
      },
      "apply": {
        "model": "swem:block/slow_feeder_gray"
      }
    }
  ]
}